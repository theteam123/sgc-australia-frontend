# Example Environment Configuration for Local Development
# Copy this file to .env.local and update the values

# Frappe ERPNext API Configuration
VITE_ERPNEXT_API_URL=http://localhost:8000
VITE_ERPNEXT_TAKTEC_API_URL=http://localhost:8000
VITE_ERPNEXT_SGCLOUD_API_URL=http://localhost:8000

# OAuth Configuration for Local Development
# Update these with your actual OAuth app credentials from Frappe
VITE_OAUTH_CLIENT_ID=local_dev_client
VITE_OAUTH_CLIENT_SECRET=local_dev_secret
VITE_OAUTH_REDIRECT_URI=http://localhost:3001/oauth-callback

# Domain-specific OAuth (using same local values for now)
VITE_OAUTH_TAKTEC_CLIENT_ID=local_dev_client
VITE_OAUTH_TAKTEC_CLIENT_SECRET=local_dev_secret
VITE_OAUTH_TAKTEC_REDIRECT_URI=http://localhost:3001/oauth-callback

VITE_OAUTH_SGCLOUD_CLIENT_ID=local_dev_client
VITE_OAUTH_SGCLOUD_CLIENT_SECRET=local_dev_secret
VITE_OAUTH_SGCLOUD_REDIRECT_URI=http://localhost:3001/oauth-callback

VITE_OAUTH_MRCES_CLIENT_ID=local_dev_client
VITE_OAUTH_MRCES_CLIENT_SECRET=local_dev_secret
VITE_OAUTH_MRCES_REDIRECT_URI=http://localhost:3001/oauth-callback

# Admin API Keys (for server-side operations)
# You can get these from Frappe > Setup > Integrations > API Access
VITE_ADMIN_API_KEY=your_admin_api_key
VITE_ADMIN_API_SECRET=your_admin_api_secret

# Claude API Key (for AI features - optional)
VITE_CLAUDE_API_KEY=your_claude_api_key

# Public Form Token (optional)
VITE_PUBLIC_FORM_TOKEN=your_public_form_token

# Development Settings
NODE_ENV=development
VITE_DEV_MODE=true 